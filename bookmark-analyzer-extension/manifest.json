{
  "manifest_version": 2,
  "name": "Bookmark Analyzer Extension",
  "version": "1.0",
  "permissions": [
    "bookmarks",
    "http://localhost:5000/"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Send Bookmarks"
  }
}
